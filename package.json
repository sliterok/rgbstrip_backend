    "aedes": "^0.47.0",
    "axios": "^0.27.2",
    "axios-cookiejar-support": "^4.0.7",
    "chroma-js": "^2.4.2",
    "eslint": "^8.57.0",
    "express": "^4.19.2",
    "grammy": "^1.22.4",
    "grammy-inline-menu": "^8.0.1",
    "nanotimer": "^0.3.15",
    "prettier": "^2.8.8",
    "rakkasjs": "0.7.0-next.47",
    "simplex-noise": "^4.0.1",
    "tough-cookie": "^4.1.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-tsconfig-paths": "^3.6.0"
  },
