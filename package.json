    "awesome-debounce-promise": "^2.1.0",
    "colorjs.io": "^0.4.5",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.3.1",
    "ring-buffer-ts": "^1.2.0",
    "telegram-webapps-types": "^1.0.5",
    "use-constant": "^1.1.1"
