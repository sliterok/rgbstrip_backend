{
  "name": "rgbstrip-new",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "rakkas --host 0.0.0.0 --port 8001",
    "build": "cross-env VITE_RSC_BUILD=1 rakkas build",
    "start": "cross-env NODE_ENV=production PORT=8001 HOST=0.0.0.0 node dist/server",
    "restart": "pm2 restart 8",
    "format": "prettier --write --ignore-unknown src",
    "test": "npm run test:typecheck && npm run test:format && npm run test:lint",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:format": "prettier --check --ignore-unknown src",
    "test:lint": "eslint . --ignore-pattern dist"
  },
  "devDependencies": {
    "@rakkasjs/eslint-config": "0.6.7",
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.3.1",
